<Window x:Class="StartTeams.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartTeams"
        mc:Ignorable="d"
        Title="Teams Starter" Height="228" Width="315">
    <StackPanel>
        <Button Content="Start Microsoft Teams" Command="{Binding StartTeamsCommand}"/>
        <Button Content="EXIT" Command="{Binding ExitAppCommand}"/>
    </StackPanel>
</Window>
